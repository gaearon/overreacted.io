{
  "private": true,
  "name": "readthatcomic",
  "version": "1.0.0",
  "description": "Read that Comic",
  "license": "MIT",
  "author": "Rossano D'Angelo <rossanodan@gmail.com>",
  "homepage": "https://readthatcomic.vercel.app",
  "bugs": {
    "url": "https://github.com/rossanodan/readthatcomic/issues"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop -p 8001",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "serve": "gatsby serve",
    "start": "gatsby develop -p 8001",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "resolutions": {
    "**/sharp": "^0.23"
  },
  "dependencies": {
    "gatsby": "^2.0.76",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-ebook": "^1.0.2",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-i18n": "^0.4.2",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "~2.8",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-autolink-headers": "2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.3",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.7.0"
  },
  "engines": {
    "node": "12.x || 14.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
